{
    "name": "diegocopat/laravel-repository-pattern",
    "description": "Laravel Repository Pattern with Service Layer - Complete setup with Jetstream, Inertia, SSR, Teams, and Spatie packages",
    "keywords": [
        "laravel",
        "repository-pattern",
        "service-layer",
        "jetstream",
        "inertia",
        "spatie",
        "diegocopat"
    ],
    "homepage": "https://github.com/diegocopat/laravel-repository-pattern",
    "license": "MIT",
    "authors": [
        {
            "name": "Diego Copat",
            "email": "your-email@example.com",
            "homepage": "https://github.com/diegocopat"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "DiegoCopat\\RepositoryPattern\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DiegoCopat\\RepositoryPattern\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DiegoCopat\\RepositoryPattern\\Providers\\RepositoryPatternServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}